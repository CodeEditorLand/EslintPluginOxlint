{
	"name": "eslint-plugin-oxlint",
	"description": "Turn off all rules already supported by oxlint",
	"exports": {
		".": {
			"default": "./index.js",
			"import": "./index.js",
			"require": "./index.cjs",
			"types": "./index.d.ts"
		},
		"./rules": {
			"default": "./rules.js",
			"import": "./rules.js",
			"require": "./rules.cjs",
			"types": "./rules.d.ts"
		}
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"generate": "tsx ./scripts/generate-rules.ts",
		"prepare": "husky",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"release": "bumpp package.json"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write",
		"*.{js,cjs,ts}": "eslint"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@types/node": "20.12.11",
		"axios": "1.6.8",
		"bumpp": "9.4.1",
		"lint-staged": "15.2.2",
		"oxlint": "0.3.2",
		"tsx": "4.9.3"
	}
}
