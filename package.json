{
	"name": "eslint-plugin-oxlint",
	"version": "0.0.1",
	"private": false,
	"description": "Turn off all rules already supported by oxlint",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/EslintPluginOxlint#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/EslintPluginOxlint/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/EslintPluginOxlint.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"exports": {
		".": {
			"default": "./index.js",
			"import": "./index.js",
			"require": "./index.cjs",
			"types": "./index.d.ts"
		},
		"./rules": {
			"default": "./rules.js",
			"import": "./rules.js",
			"require": "./rules.cjs",
			"types": "./rules.d.ts"
		}
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"generate": "tsx ./scripts/generate-rules.ts",
		"prepare": "husky",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"release": "bumpp package.json"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write",
		"*.{js,cjs,ts}": "eslint"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.7",
		"axios": "1.6.8",
		"bumpp": "9.4.1",
		"lint-staged": "15.2.2",
		"oxlint": "0.3.1",
		"tsx": "4.8.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
